{
    "package": {
        "name": "python-gcg-xenial",
        "repo": "deb-oss",
        "subject": "weakcamel",
        "desc": "Git Changelog Generator",
        "website_url": "https://pypi.org/project/gcg/",
        "issue_tracker_url": "https://github.com/nokia/git-changelog-generator/issues",
        "vcs_url": "https://github.com/nokia/git-changelog-generator",
        "licenses": [
            "BSD 3-Clause"
        ],
        "labels": [
            "deb",
            "git",
            "changelog",
            "python"
        ],
        "public_download_numbers": true,
        "public_stats": true
    },

    "version": {
        "name": "${version}",
        "desc": "This is a version",
        "released": "${release_date}",
        "vcs_tag": "${version}",
        "gpgSign": false
    },

    "files": [
        {
            "includePattern": "deb_dist/(.*\\.deb)",
            "excludePattern": ".*/do-not-deploy/.*",
            "uploadPattern": "pool/g/python-gcg/$1",
            "matrixParams:" : {
                "deb_distribution": "xenial",
                "deb_component": "main",
                "deb_architecture": "all"
            }
        }
    ],
    "publish": true
}
